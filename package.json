{
  "name": "j-m.github.io",
  "version": "1.0.0",
  "description": "My website",
  "main": "index.js",
  "devDependencies": {
    "gh-pages": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.31.0",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-livereload": "^1.0.4",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.1.1",
    "sirv-cli": "^1.0.0-next.3",
    "svelte": "^3.18.1"
  },
  "dependencies": {
    "rollup-plugin-terser": "^5.1.2",
    "tagable": "^0.4.1"
  },
  "scripts": {
    "build": "rollup -c src/rollup.config.js",
    "build:dev": "rollup -c src/rollup.config.js -w",
    "deploy": "gh-pages -d public",
    "open": "sirv ./public --single 404.html",
    "open:dev": "sirv ./public --single 404.html --dev",
    "publish": "run-s build deploy",
    "start": "run-s build open",
    "start:dev": "run-p build:dev open:dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/j-m/j-m.github.io.git"
  },
